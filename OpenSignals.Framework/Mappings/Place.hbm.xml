<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="OpenSignals.Framework.Places.Place, OpenSignals.Framework" table="places">
    <id name="PlaceID" column="p_id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Name" column="p_name" type="String"></property>
    <property name="Status" column="p_status" type="Boolean"></property>
    <property name="Open311URL" column="p_open311_url" type="String"></property>
    <property name="Open311ApiKey" column="p_open311_api_key" type="String"></property>
    <property name="Open311CityID" column="p_open311_city_id" type="String"></property>
    <property name="MapZoom" column="p_zoom" type="int"></property>
    <property name="GeolocationInfo" column="p_geo" type="OpenSignals.Framework.API.Google.Geometry, OpenSignals.Framework"></property>
  </class>
</hibernate-mapping>
